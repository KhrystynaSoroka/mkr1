
void main() {
    Student[] students = {
            new Student("Христина", "Сорока", "КНС-11", 5),
            new Student("Марія", "Коваленко", "IT-12", 4.7),
            new Aspirant("Олег", "Бондар", "AS-21", 5, "Розробка програми для обліку виробничих процесів підприємства")
    };

    for (Student s : students) {
        s.printInfo();
    }
}

